<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.DevLevelChoose">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/vibe_post_top_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D3E4CD"
        android:layout_marginTop="0dp"
        android:paddingHorizontal="15dp"
        android:paddingTop="20dp"
        android:paddingBottom="16dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-16dp">

        <ImageButton
            android:id="@+id/start_back_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@null"
            android:contentDescription="@string/app_name"
            android:src="@drawable/notice_ico"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/vibe_post_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/dev_level"
            android:textColor="@color/black"
            android:textSize="17sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <ImageButton
            android:id="@+id/start_menu_btn"
            android:layout_width="38dp"
            android:layout_height="15dp"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="320dp"
            android:background="@null"
            android:contentDescription="@string/app_name"
            android:src="@drawable/menu_ico"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.509" />

    </androidx.constraintlayout.widget.ConstraintLayout>



    <EditText
        android:id="@+id/person1"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        app:layout_constraintStart_toEndOf="@+id/icoperson1"
        app:layout_constraintTop_toTopOf="@+id/icoperson1" />

    <ImageView
        android:id="@+id/icoperson1"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginTop="136dp"
        android:background="@drawable/person_ico"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vibe_post_top_bar_layout" />

    <Button
        android:id="@+id/mid_button1"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_mid_selector"

        app:layout_constraintStart_toEndOf="@id/low_button1"
        app:layout_constraintTop_toTopOf="@id/person1" />

    <Button
        android:id="@+id/low_button1"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/level_button_low_selector"

        app:layout_constraintStart_toEndOf="@id/person1"
        app:layout_constraintTop_toTopOf="@id/person1" />

    <Button
        android:id="@+id/high_button1"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_high_selector"

        app:layout_constraintStart_toEndOf="@id/mid_button1"
        app:layout_constraintTop_toTopOf="@id/person1" />

    <!-- Second person -->
    <ImageView
        android:id="@+id/icoperson2"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/person_ico"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/person1" />

    <EditText
        android:id="@+id/person2"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        app:layout_constraintStart_toEndOf="@+id/icoperson2"
        app:layout_constraintTop_toTopOf="@+id/icoperson2" />

    <Button
        android:id="@+id/low_button2"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/level_button_low_selector"
        app:layout_constraintStart_toEndOf="@+id/person2"
        app:layout_constraintTop_toTopOf="@+id/person2" />

    <Button
        android:id="@+id/mid_button2"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_mid_selector"
        app:layout_constraintStart_toEndOf="@+id/low_button2"
        app:layout_constraintTop_toTopOf="@+id/person2" />

    <Button
        android:id="@+id/high_button2"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_high_selector"
        app:layout_constraintStart_toEndOf="@+id/mid_button2"
        app:layout_constraintTop_toTopOf="@+id/person2" />

    <!-- Third person -->
