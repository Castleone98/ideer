        android:id="@+id/icoperson3"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/person_ico"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/person2" />

    <EditText
        android:id="@+id/person3"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        app:layout_constraintStart_toEndOf="@+id/icoperson3"
        app:layout_constraintTop_toTopOf="@+id/icoperson3" />

    <Button
        android:id="@+id/low_button3"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/level_button_low_selector"
        app:layout_constraintStart_toEndOf="@+id/person3"
        app:layout_constraintTop_toTopOf="@+id/person3" />

    <Button
        android:id="@+id/mid_button3"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_mid_selector"
        app:layout_constraintStart_toEndOf="@+id/low_button3"
        app:layout_constraintTop_toTopOf="@+id/person3" />

    <Button
        android:id="@+id/high_button3"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_high_selector"
        app:layout_constraintStart_toEndOf="@+id/mid_button3"
        app:layout_constraintTop_toTopOf="@+id/person3" />

    <!-- Fourth person -->
