        android:id="@+id/icoperson4"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/person_ico"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/person3" />

    <EditText
        android:id="@+id/person4"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        app:layout_constraintStart_toEndOf="@+id/icoperson4"
        app:layout_constraintTop_toTopOf="@+id/icoperson4" />

    <Button
        android:id="@+id/low_button4"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/level_button_low_selector"
        app:layout_constraintStart_toEndOf="@+id/person4"
        app:layout_constraintTop_toTopOf="@+id/person4" />

    <Button
        android:id="@+id/mid_button4"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_mid_selector"
        app:layout_constraintStart_toEndOf="@+id/low_button4"
        app:layout_constraintTop_toTopOf="@+id/person4" />

    <Button
